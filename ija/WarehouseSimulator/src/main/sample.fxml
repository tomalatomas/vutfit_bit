<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<GridPane alignment="center" hgap="10" prefWidth="1080.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.202" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane maxWidth="1.7976931348623157E308" prefHeight="700.0" prefWidth="720.0" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
         <children>
            <VBox maxWidth="-Infinity" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <HBox id="resButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="1100.0" spacing="10.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Button fx:id="helpButton" mnemonicParsing="false" onAction="#displayHelp" prefHeight="50.0" prefWidth="50.0" style="-fx-background-image: url('file:./data/images/help.png'); -fx-background-color: transparent;">
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                           <padding>
                              <Insets left="20.0" />
                           </padding>
                        </Button>
                        <Button fx:id="manualAdd" mnemonicParsing="false" onAction="#manualWindow" prefHeight="50.0" prefWidth="50.0" style="-fx-background-image: url('file:./data/images/hand.png'); -fx-background-color: transparent;" />
                        <Button fx:id="initMapFile" mnemonicParsing="false" onAction="#loadMapFile" prefHeight="50.0" prefWidth="50.0" style="-fx-background-image: url('file:./data/images/folder.png'); -fx-background-color: transparent;" />
                        <Button fx:id="initMap1" mnemonicParsing="false" onAction="#loadMap1" prefHeight="50.0" prefWidth="50.0" style="-fx-background-image: url('file:./data/images/map1.png'); -fx-background-color: transparent;" />
                        <Button fx:id="initMap2" mnemonicParsing="false" onAction="#loadMap2" prefHeight="50.0" prefWidth="50.0" style="-fx-background-image: url('file:./data/images/map2.png'); -fx-background-color: transparent;" />
                        <Button mnemonicParsing="false" onAction="#restart" prefHeight="50.0" prefWidth="50.0" style="-fx-background-image: url('file:./data/images/restart.png'); -fx-background-color: transparent;">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin></Button>
                        <Button mnemonicParsing="false" onAction="#fastForward" prefHeight="50.0" prefWidth="50.0" style="-fx-background-image: url('file:./data/images/fast.png'); -fx-background-color: transparent;" />
                        <Button fx:id="addRequestButton" mnemonicParsing="false" onAction="#addRequest" prefHeight="50.0" prefWidth="50.0" style="-fx-background-image: url('file:./data/images/add.png'); -fx-background-color: transparent;" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" top="20.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox maxHeight="-Infinity" maxWidth="1080.0" minHeight="-Infinity" minWidth="1080.0" prefHeight="650.0" prefWidth="1080.0">
                     <children>
                        <TableView fx:id="tableview" maxHeight="640.0" prefHeight="640.0" prefWidth="440.0">
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                          <columns>
                            <TableColumn id="name" fx:id="nameCol" editable="false" prefWidth="264.0" sortable="false" text="Name" />
                              <TableColumn id="inStock" fx:id="inStockCol" prefWidth="96.0" text="In Stock" />
                             <TableColumn id="quantity" fx:id="requestQuantityCol" onEditCommit="#onEditChange" prefWidth="79.0" sortable="false" text="Quantity" />
                          </columns>
                           <HBox.margin>
                              <Insets left="30.0" />
                           </HBox.margin>
                        </TableView>
                        <Pane id="mapPane" fx:id="mapPane" maxHeight="640.0" maxWidth="640.0" minHeight="640.0" minWidth="640.0" prefHeight="640.0" prefWidth="640.0" style="-fx-background-color: FFFFFF;" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</GridPane>
